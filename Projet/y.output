Grammaire

    0 $accept: algorithm_list $end

    1 algorithm_list: algorithm_list algorithm
    2               | algorithm

    3 algorithm: TEXSCI_BEGIN '{' ID '}' '\n' zone_declarations zone_instructions TEXSCI_END

    4 zone_instructions: list_instructions
    5                  | %empty

    6 list_instructions: instruction '\n' list_instructions
    7                  | instruction '\n'

    8 instruction: instruction_affectation

    9 instruction_affectation: '$' ID operateur_affectation expression_arithmetique '$' EOI

   10 expression_arithmetique: expression_arithmetique operateur_m expression_arithmetique_t
   11                        | expression_arithmetique_t

   12 expression_arithmetique_t: expression_arithmetique_t operateur_f expression_arithmetique_f
   13                          | expression_arithmetique_f

   14 expression_arithmetique_f: '(' expression_arithmetique ')'
   15                          | valeur
   16                          | ID

   17 operateur_f: DIV
   18            | MULT

   19 operateur_m: PLUS
   20            | MINUS

   21 operateur_affectation: LEFTARROW

   22 zone_declarations: zone_declaration_constante zone_declaration_input zone_declaration_output zone_declaration_global zone_declaration_local BLANKLINE '\n'

   23 zone_declaration_constante: DECLARECONSTANT '{' '$' suite_declarations_constante '$' '}' '\n'
   24                           | %empty

   25 zone_declaration_input: DECLAREINPUT '{' '$' suite_declarations_variable '$' '}' '\n'
   26                       | %empty

